// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+l ctrl+d",
        "command": "editor.action.goToDeclaration",
        "when": "editorFocus && vim.mode == 'Normal'"
    },
    {
        "key": "ctrl+l ctrl+f",
        "command": "editor.action.codeAction",
        "args": {
            "kind": "source.fixAll.tslint",
            "apply": "first"
        }
    }
]
